/* Ler um número inteiro, ◦ verificar se o número corresponde a um mês válido no calendário
escrever o nome do mês, senão escrever uma mensagem 'Mês Inválido'.*/

//Inclusão de bibliotecas
#include <stdio.h>
#include <stdlib.h>
//Função principal
int main()
{
//Declaração de variáveis
int num;

//Entrada de dados
printf("Digite o numero do mes\n");
scanf("%d", &num);

//Cálculo
switch(num)
    {
    case 1: 
    printf("Mes de janeiro digitado");
        break;
    case 2: 
        printf("Mes de fevereiro digitado");
        break;
    case 3: 
        printf("Mes de marco digitado");
        break;
    case 4: 
        printf("Mes de abril digitado");
        break;
    case 5: 
        printf("Mes de maio digitado");
        break;
    case 6:    
        printf("Mes de junho digitado");
        break;
    case 7: 
        printf("Mes de julho digitado");
        break;
    case 8: 
        printf("Mes de agosto digitado");
        break;
    case 9: 
        printf("Mes de setembro digitado");
        break;
    case 10: 
        printf("Mes de outubro digitado");
        break;
    case 11: 
        printf("Mes de novembro digitado");
        break;
    case 12:    
        printf("Mes de dezembro digitado");
        break;
    default:
        printf("Mes invalido\n");
        exit(0);
    }
//Fim da função principal              
return 0;
}
